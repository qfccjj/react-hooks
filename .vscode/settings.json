{
	"stylelint.enable": true,
	"editor.codeActionsOnSave": {
		"source.fixAll.stylelint": "explicit"
	},
	"stylelint.validate": [
		"css",
		"less",
		"postcss",
		"scss",
		"tsx",
		"html"
	],
	"cSpell.words": [
		"antd",
		"breakline",
		"browserslist",
		"cnpm",
		"commitlint",
		"doclock",
		"Gitee",
		"loglevel",
		"Prefixs",
		"styl",
		"stylelint"
	],
	"files.eol": "\n"
}
